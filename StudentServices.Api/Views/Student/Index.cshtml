

@model List<StudentServices.Api.Models.student1>
<h2>Students</h2>

<a href="/Student/Create">Create New</a>

<table border="1" cellpadding="8">
    <tr>
        <th>#</th>
        <th>Name</th>
        <th>Age</th>
        <th>Action</th>
    </tr>
    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@i</td>
            <td>@Model[i].Name</td>
            <td>@Model[i].Age</td>
            <td>
                <a href="/Student/Details/@i">Details</a>
            </td>
        </tr>
    }
</table>